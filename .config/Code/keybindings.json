// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+r ctrl+c",
        "command": "extension.removeAllComments"
    },
    {
        "key": "ctrl+r ctrl+e",
        "command": "remove-empty-lines.inDocument"
    },
    {
        "key": "ctrl+alt+up",
        "command": "block-travel.jumpDown",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+down",
        "command": "-block-travel.jumpDown",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+down",
        "command": "block-travel.jumpUp",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+up",
        "command": "-block-travel.jumpUp",
        "when": "editorTextFocus"
    }
]